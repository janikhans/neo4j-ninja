<div class="col-md-12">
	<div class="content-box-large">
		<div class="panel-heading">
		<div class="panel-title">Vehicle Years</div>

		<div class="panel-options">
      <%= link_to 'New Vehicle Year', new_vehicle_year_path %>
			<a href="#" data-rel="collapse"><i class="glyphicon glyphicon-refresh"></i></a>
			<a href="#" data-rel="reload"><i class="glyphicon glyphicon-cog"></i></a>
		</div>
	</div>
		<div class="panel-body">
			<table class="table table-striped">
        <thead>
          <tr>
            <th>#</th>
            <th>Year</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @vehicle_years.each do |vehicle_year| %>
            <tr>
              <td><%= vehicle_year.id %></td>
              <td><%= vehicle_year.year %></td>
              <td><%= link_to 'Show', vehicle_year %></td>
              <td><%= link_to 'Edit', edit_vehicle_year_path(vehicle_year) %></td>
              <td><%= link_to 'Destroy', vehicle_year, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
		</div>
	</div>
</div>
